/////////////////////////////////////////////////////////////////////
//
// reach the good target POMDP
//
// Author: Nicolas Drougard (nicolas.drougard [at] onera.fr)
//
// In a grid, a robot (R) has to reach a target (A).
// Successive locations of (R) are fully observable.
// Two targets, T1 and T2 are in this grid,
// and their locations are known.
// One target is (A), and the other is (B).
// However, the targets natures, (A) or (B), are unknown,
// and the aim is to reach target (A).
// At each process step, the robot analyzes pictures of each target
// which produces a guess for targets natures:
// the two targets can be observed as (A), which corresponds to the observation (oAA)
// or only T1 (oAB), or only T2 (oBA) or no target (oBB).
//
// As an ignorant prior, the initial belief b_0 is: p(T1=A) = p(T2=A) = 0.5
//
// * * * * * * * * * * * *
// * T1                  *
// *                     *
// *                     *
// *                     *
// *                     *
// *                     *
// *                     *
// * R                T2 *
// * * * * * * * * * * * *
//
/////////////////////////////////////////////////////////////////////

domain RTGT_pomdp {
	requirements = {
		reward-deterministic,
		partially-observed
	};

	pvariables {

		//States
		robot-loc-state-0 : {state-fluent, bool, default = false};
		robot-loc-state-1 : {state-fluent, bool, default = false};
		robot-loc-state-2 : {state-fluent, bool, default = false};
		robot-loc-state-3 : {state-fluent, bool, default = false};
		robot-loc-state-4 : {state-fluent, bool, default = false};
		robot-loc-state-5 : {state-fluent, bool, default = false};
		robot-loc-state-6 : {state-fluent, bool, default = false};
		targetOneIsA : {state-fluent, bool, default = false};

		//Observations
		robot-loc-obs-0 : {observ-fluent, bool};
		robot-loc-obs-1 : {observ-fluent, bool};
		robot-loc-obs-2 : {observ-fluent, bool};
		robot-loc-obs-3 : {observ-fluent, bool};
		robot-loc-obs-4 : {observ-fluent, bool};
		robot-loc-obs-5 : {observ-fluent, bool};
		robot-loc-obs-6 : {observ-fluent, bool};
		obs-target-one : {observ-fluent, bool};
		obs-target-two : {observ-fluent, bool};

		// Actions
		move-north : {action-fluent, bool, default = false};
		move-south : {action-fluent, bool, default = false};
		move-east  : {action-fluent, bool, default = false};
		move-west  : {action-fluent, bool, default = false};
		stay : {action-fluent, bool, default = false};

	};


	cpfs {

		robot-loc-obs-0 = KronDelta(robot-loc-state-0);
		robot-loc-obs-1 = KronDelta(robot-loc-state-1);
		robot-loc-obs-2 = KronDelta(robot-loc-state-2);
		robot-loc-obs-3 = KronDelta(robot-loc-state-3);
		robot-loc-obs-4 = KronDelta(robot-loc-state-4);
		robot-loc-obs-5 = KronDelta(robot-loc-state-5);
		robot-loc-obs-6 = KronDelta(robot-loc-state-6);

		obs-target-one = if ( targetOneIsA ^ ~robot-loc-state-0 ^ ~robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.7032848298)
				else if ( ~targetOneIsA  ^ ~robot-loc-state-0 ^ ~robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.2967151702)
				else if ( targetOneIsA ^  robot-loc-state-0 ^ ~robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.7021620461)
				else if ( ~targetOneIsA  ^  robot-loc-state-0 ^ ~robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.2978379539)
				else if ( targetOneIsA ^ ~robot-loc-state-0 ^  robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.6988704589)
				else if ( ~targetOneIsA  ^ ~robot-loc-state-0 ^  robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.3011295411)
				else if ( targetOneIsA ^  robot-loc-state-0 ^  robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
					then Bernoulli(0.6936252907)
				else if ( ~targetOneIsA  ^  robot-loc-state-0 ^  robot-loc-state-1 ^ ~robot-loc-state-2 ^ ~robot-loc-state-3 ^ ~robot-loc-state-4 ^ ~robot-loc-state-5 ^ ~robot-loc-state-6 )
